<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link width="" rel="shortcut icon" href="./images/Nani.png" />

	<title> CAMISETAS NANI</title>
	<!-- Font Awesome Stylesheet -->
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/css/all.min.css">
	<!-- Bootstrap Grid Stylesheet -->
	<link rel="stylesheet" href="/stylesheets/css/app.css">
	<link rel="stylesheet" href="/stylesheets/css/app.css">
	<link rel="stylesheet" href="/stylesheets/bootstrap-grid.min.css">
	<!-- Own Stylesheet -->
	<link rel="stylesheet" href="/stylesheets/app.css">
</head>

<body>
	<!-- Header -->
	<%- include('partials/header') %>
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Registrate</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<h2 class="col-12"> <form action="/register" method="GET">
				<label for="email">Email:</label>
				<input type="email" name="email" id="email" required><br>
			  
				<label for="usuario">Usuario:</label>
				<input type="text" name="usuario" id="usuario" required><br>
			  
				<label for="password">Contraseña:</label>
				<input type="password" name="password" id="password" required><br>
			  
				<label for="fecha">Fecha de nacimiento:</label>
				<input type="date" name="fecha" id="fecha" required><br>
			  
				<label for="documento">N° de documento:</label>
				<input type="number" name="documento" id="documento" required><br>
			  
				<label for="foto">Foto de perfil (URL):</label>
				<input type="text" name="foto" id="foto"><br>
			  
				<button type="submit">Registrarme</button>
			  </form>
			  
			</h2>


	<!-- footer -->
	<%- include('partials/footer') %>
	<!-- /footer -->
</body>

</html>